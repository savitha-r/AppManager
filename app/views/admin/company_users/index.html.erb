<% if @c_user and @c_user.errors.any? %>
		<ul>
  			<% @c_user.errors.full_messages.each do |msg| %>
    			<li><%= msg %></li>
  			<% end %>
		</ul>
	<% end %>

<% @company_users.each do |c_user| %>
<p> Name: <%= c_user.user.name %></p>

<%= form_for c_user, :as => :company_user, :url => admin_company_save_path(c_user), :method => :put, :html => {:multipart => true } do |f| %>



<%= f.label :role %><br>
<%= f.select 'role', options_for_select(["member", "developer", "admin"].each_with_index.map {|name, index| [name,name]}, c_user.role) %>
<%= f.submit "Save", style: "width:120px; height:35px; background-color:#c25b39; border:1px solid #c25b39; color:#FFF; font-size:17px; float:left; margin-right:180px;" %>
<% end %>


<% end %>